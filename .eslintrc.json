{
    "plugins": [
        "prettier"
    ],
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends":[
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 1, // Means warning
        "prettier/prettier": 2 // Means error
    }
}
